{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ncmfse/post-video-player",
  "version": "0.1.0",
  "title": "Ncmaz Post Video Player",
  "category": "ncmfse",
  "usesContext": [
    "postId",
    "postType"
  ],
  "description": "A block to display video/iframe player for posts with video format.",
  "example": {},
  "attributes": {},
  "supports": {
    "anchor": false,
    "html": false,
    "color": {
      "gradients": true,
      "__experimentalDefaultControls": {
        "background": true
      }
    },
    "shadow": true,
    "spacing": {
      "padding": true,
      "margin": true,
      "__experimentalDefaultControls": {
        "padding": true
      }
    },
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true,
      "__experimentalDefaultControls": {
        "color": true,
        "radius": true,
        "style": true,
        "width": true
      }
    },
    "interactivity": true
  },
  "textdomain": "default",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScriptModule": "file:./view.js"
}